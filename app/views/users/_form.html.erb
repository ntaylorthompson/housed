<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>

  <div class="field">
    <%= f.label "Header URL" %><br />
    <%= f.text_field :header %>
  </div>
  <div class="field">
    <%= f.label "Website URL" %><br />
    <%= f.text_field :website %>
  </div>
  <div class="field">
    <%= f.label "MP3 1 URL" %><br />
    <%= f.text_field :mp3_1_url %>
  </div>
  <div class="field">
    <%= f.label "MP3 2 URL" %><br />
    <%= f.text_field :mp3_2_url %>
  </div>

  <div class="field">



    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation, "Confirmation" %>
    <%= f.password_field :password_confirmation %>


  </div>

  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
